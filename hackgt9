from PIL import Image

image=Image.open('nutrition.jpeg')
width=image.width
height=image.height
difference=abs(width-height)
im1=image.crop((0,height*0.65,width,height))

fixed_height=height
height_percent=(fixed_height/float(image.size[1]))
width_size=int((float(image.size[0])*float(height_percent)))
im1=im1.resize((2*width_size,height))
im1.show()



